        <name>Insert Record</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var requestBody = request.body.data;
    var countryName = requestBody.countryName;
    var htmlSummaryEN = requestBody.htmlSummaryEN;
    var decodedString = new GlideStringUtil().unEscapeHTML(htmlSummaryEN);
    var countryGR = new GlideRecord('u_countries');
    countryGR.initialize();
    countryGR.u_country_name = countryName;
    countryGR.u_nation_famous_for = decodedString;
    var responseObj = countryGR.insert();
    response.body = JSON.stringify(responseObj);

})(request, response);]]></operation_script>
